{
	"name": "ts-template",
	"version": "1.0.0",
	"description": "My own template for typescript projects",
	"main": "index.js",
	"scripts": {
		"build": "tsc --build",
		"lint": "eslint --config ./.eslintrc --ignore-path ./.eslintignore --ext .js,.ts .",
		"postinstall": "husky install",
		"prettier": "prettier --config ./.prettierrc --write \"**/*.{js,json,md,ts}\"",
		"prettier:check": "prettier --check --config ./.prettierrc \"**/*.{js,json,md,ts}\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dmpzh/ts-template.git"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/dmpzh/ts-template/issues"
	},
	"homepage": "https://github.com/dmpzh/ts-template#readme",
	"devDependencies": {
		"@types/node": "^17.0.30",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"eslint": "^8.14.0",
		"eslint-config-prettier": "^8.5.0",
		"husky": "^7.0.4",
		"prettier": "^2.6.2",
		"typescript": "^4.6.3"
	},
	"dependencies": {
		"chalk": "^4.0.0"
	}
}
